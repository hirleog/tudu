{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/ngsw-worker.js",
          "/service-worker.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/**",
          "https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/**/*.svg",
          "/**/*.cur",
          "/**/*.jpg",
          "/**/*.jpeg",
          "/**/*.png",
          "/**/*.apng",
          "/**/*.webp",
          "/**/*.avif",
          "/**/*.gif",
          "/**/*.otf",
          "/**/*.ttf",
          "/**/*.woff",
          "/**/*.woff2",
          "/**/*.eot"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["/api/**", "/notifications/**"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "10s",
        "strategy": "performance"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/assets/**",
    "!/api/**",
    "!/**/*.json"
  ],
  "navigationRequestStrategy": "freshness",
  "notifications": {
    "enabled": true
  }
}
